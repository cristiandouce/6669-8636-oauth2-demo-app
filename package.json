{
  "name": "6669-8636-oauth2-demo-app",
  "version": "1.0.0",
  "description": "Aplicación empleada como demo de OAuth2 para el curso de Criptografía y Seguridad Informática [6669][8636]",
  "dependencies": {
    "axios": "0.24.0",
    "body-parser": "1.19.0",
    "bootstrap": "^5.1.3",
    "connect-flash": "0.1.1",
    "connect-mongo": "4.6.0",
    "cookie-parser": "1.4.6",
    "ejs": "^3.1.6",
    "express": "4.17.1",
    "express-oauth-server": "2.0.0",
    "express-session": "1.17.2",
    "migrate-mongoose": "4.0.0",
    "mongoose": "6.0.14",
    "on-finished": "2.3.0",
    "passport": "0.5.0",
    "passport-local": "1.0.0",
    "passport-oauth2": "1.6.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "nodemon": "2.0.15"
  },
  "scripts": {
    "start": "./bin/start.sh",
    "start:dev": "NODE_ENV=development ./bin/start.sh",
    "start:web": "node ./packages/web-server/index.js",
    "start:web:dev": "nodemon ./packages/web-server/index.js",
    "start:authz": "node ./packages/authz-server/index.js",
    "start:authz:dev": "nodemon ./packages/authz-server/index.js",
    "migrations": "migrate --config ./packages/authz-server/lib/db/mongoose/migrations/_config.js",
    "migrations:run": "migrate --config ./packages/authz-server/lib/db/mongoose/migrations/_config.js up",
    "migrations:list": "migrate --config ./packages/authz-server/lib/db/mongoose/migrations/_config.js list",
    "migrations:create": "migrate --config ./packages/authz-server/lib/db/mongoose/migrations/_config.js create",
    "migrations:teardown": "migrate --config ./packages/authz-server/lib/db/mongoose/migrations/_config.js down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cristiandouce/6669-8636-oauth2-demo-app.git"
  },
